if(start<passive_index_file.length-1){start++;}
$("#main_container").removeClass("loading");showSiteDescriptionOnHover();scrollShadows();getIp();showCategoryIcons();},});}
loadDesktopCategories();}else{if($(window).width()>991&&typeof passive_index_file!=="undefined"){function loadDesktopCategories(){flag=true;$("#main_container").addClass("loading");$.ajax({url:"/includes/pi/"+passive_index_file,type:"GET",contentType:"application/json",dataType:"json",success:function(data){if(typeof data!=="undefined"){for(var i=0;i<data.length;i++){var lastInColumn=$("div[data-column="+(i+1)+"]:last");if(lastInColumn.length>0&&data[i].length>0){$(lastInColumn).after(data[i]);}}}
if(start<passive_index_file.length-1){start++;}
myObserver.unobserve(myEls[0]);$("#main_container").removeClass("loading");showSiteDescriptionOnHover();scrollShadows();getIp();showCategoryIcons();},});}
var myObserver=new IntersectionObserver(function(elements){elements.forEach(function(index){if(index.intersectionRatio>0){loadDesktopCategories();}});});var myEls=$(".observer-block");if(myEls.length){myObserver.observe(myEls[0]);}
if(myEls.offsetTop<window.scrollY){loadDesktopCategories();}}
