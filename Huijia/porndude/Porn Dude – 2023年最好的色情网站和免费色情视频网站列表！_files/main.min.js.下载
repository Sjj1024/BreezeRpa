function showCategoryIcons(){var $icons=$('.inline-icon');function callback(entries,observer){entries.forEach(function(entry){if(entry.isIntersecting){$(entry.target).addClass('visible');observer.unobserve(entry.target);}})}
if('IntersectionObserver'in window){var intersectionObserver=new IntersectionObserver(callback)
$icons.each(function(index,icon){intersectionObserver.observe(icon);});}}
